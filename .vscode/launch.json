  "version": "0.2.0",
  "configurations": [
   
    {
      "name": "Debug main.ts with tsx",
      "type": "pwa-node",
      "request": "launch",
      "skipFiles": [
          "<node_internals>/**"
      ],
      "runtimeExecutable": "pnpm",
      "runtimeArgs": [
          "exec",
          "tsx"
      ],
      "args": [
          "${workspaceFolder}/src/main.ts"
      ],
      "cwd": "${workspaceFolder}"
    }
  ]
}